generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env()
}

model Click {
  id        Int      @id @default(autoincrement())
  key       String
  type      String
  count     Int      @default(1)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@unique([type, key])
  @@map("clicks")
}
